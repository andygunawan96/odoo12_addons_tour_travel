<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for transport.booking -->
        <record id="seq_transport_booking" model="ir.sequence">
            <field name="name">Transport Booking</field>
            <field name="code">transport.booking</field>
            <field name="prefix">TB.%(y)s%(month)s%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
