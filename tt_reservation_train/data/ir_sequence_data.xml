<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for transport.booking -->
        <record id="seq_transport_booking" model="ir.sequence">
            <field name="name">Reservation Train</field>
            <field name="code">tt.reservation.train</field>
            <field name="prefix">TN.%(y)s%(month)s%(day)s%(h24)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
