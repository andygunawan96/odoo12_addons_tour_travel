<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Sequences for Billing Statement -->
    <record id="seq_billing_list" model="ir.sequence">
        <field name="name">Billing Statement</field>
        <field name="code">tt.billing.statement</field>
        <field name="prefix">BS.%(range_year)s%(range_month)s%(range_day)s%(range_sec)s</field>
        <field name="padding">2</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
