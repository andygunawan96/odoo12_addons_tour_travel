<odoo>
    <record id="tt_reservation_tour_for_ho_user" model="ir.rule">
        <field name="name">Reservation Tour For HO User</field>
        <field name="model_id" ref="model_tt_reservation_tour"/>
        <field name="domain_force">[('ho_id','=',user.ho_id.id)]</field>
        <field name="groups" eval="[(4,ref('tt_base.group_tt_tour_travel'))]"/>
    </record>

    <record id="tt_reservation_tour_for_agent_user" model="ir.rule">
        <field name="name">Reservation Tour For Agent User</field>
        <field name="model_id" ref="model_tt_reservation_tour"/>
        <field name="domain_force">[('agent_id','=',user.agent_id.id)]</field>  <!--['|',('agent_id','=',user.agent_id.id),('sub_agent_id','=',user.agent_id.id)]-->  <!--[('agent_id','=',user.agent_id.id)]-->
        <field name="groups" eval="[(4,ref('tt_base.group_tt_agent_user'))]"/>  <!--,(4,ref('tt_base.group_tt_sub_agent_user_operator'))-->
    </record>

    <record id="tt_reservation_tour_for_corpor_user" model="ir.rule">
        <field name="name">Reservation Tour For Corporate User</field>
        <field name="model_id" ref="model_tt_reservation_tour"/>
        <field name="domain_force">[('agent_id','=',user.agent_id.id),('customer_parent_id','=',user.customer_parent_id.id)]</field>  <!--['|',('agent_id','=',user.agent_id.id),('sub_agent_id','=',user.agent_id.id)]-->  <!--[('agent_id','=',user.agent_id.id)]-->
        <field name="groups" eval="[(4,ref('tt_base.group_tt_corpor_user'))]"/>  <!--,(4,ref('tt_base.group_tt_sub_agent_user_operator'))-->
    </record>

    <record id="tt_request_tour_for_ho_user" model="ir.rule">
        <field name="name">Request Tour For HO User</field>
        <field name="model_id" ref="model_tt_reservation_tour"/>
        <field name="domain_force">[('ho_id','=',user.ho_id.id)]</field>
        <field name="groups" eval="[(4,ref('tt_base.group_tt_tour_travel'))]"/>
    </record>

    <record id="tt_request_tour_for_agent_user" model="ir.rule">
        <field name="name">Request Tour For Agent User</field>
        <field name="model_id" ref="model_tt_request_tour"/>
        <field name="domain_force">[('agent_id','=',user.agent_id.id)]</field>  <!--['|',('agent_id','=',user.agent_id.id),('sub_agent_id','=',user.agent_id.id)]-->  <!--[('agent_id','=',user.agent_id.id)]-->
        <field name="groups" eval="[(4,ref('tt_base.group_tt_agent_user'))]"/>  <!--,(4,ref('tt_base.group_tt_sub_agent_user_operator'))-->
    </record>

    <record id="tt_request_tour_for_corpor_user" model="ir.rule">
        <field name="name">Request Tour For Corporate User</field>
        <field name="model_id" ref="model_tt_request_tour"/>
        <field name="domain_force">[('agent_id','=',user.agent_id.id)]</field>  <!--['|',('agent_id','=',user.agent_id.id),('sub_agent_id','=',user.agent_id.id)]-->  <!--[('agent_id','=',user.agent_id.id)]-->
        <field name="groups" eval="[(4,ref('tt_base.group_tt_corpor_user'))]"/>  <!--,(4,ref('tt_base.group_tt_sub_agent_user_operator'))-->
    </record>

    <record id="tt_master_tour_for_ho_user" model="ir.rule">
        <field name="name">Master Tour For HO User</field>
        <field name="model_id" ref="model_tt_master_tour"/>
        <field name="domain_force">['|','|',('owner_ho_id','=',user.ho_id.id),('ho_ids','=',user.ho_id.id),('ho_ids','=',False)]</field>
        <field name="groups" eval="[(4,ref('tt_base.group_tt_tour_travel'))]"/>
    </record>

    <record id="tt_master_tour_type_for_ho_user" model="ir.rule">
        <field name="name">Master Tour Type For HO User</field>
        <field name="model_id" ref="model_tt_master_tour_type"/>
        <field name="domain_force">[('ho_id','=',user.ho_id.id)]</field>
        <field name="groups" eval="[(4,ref('tt_base.group_tt_tour_travel'))]"/>
    </record>
</odoo>