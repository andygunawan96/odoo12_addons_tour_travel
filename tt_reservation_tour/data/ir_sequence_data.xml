<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_reservation_tour" model="ir.sequence">
            <field name="name">Reservation Tour</field>
            <field name="code">tt.reservation.tour</field>
            <field name="prefix">TR.%(range_y)s%(range_month)s%(range_day)s%(range_sec)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_reservation_rodextrip_tour" model="ir.sequence">
            <field name="name">Reservation Orbisway Tour</field>
            <field name="code">rodextrip.tour.reservation.code</field>
            <field name="prefix">MTR.%(range_y)s%(range_month)s%(range_day)s%(range_sec)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_tour_code_group" model="ir.sequence">
            <field name="name">Group Tour Code</field>
            <field name="code">master.tour.code.group</field>
            <field name="prefix">GRDX%(range_y)s%(range_month)s%(range_sec)s</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_tour_code_fit" model="ir.sequence">
            <field name="name">FIT Tour Code</field>
            <field name="code">master.tour.code.fit</field>
            <field name="prefix">FRDX%(range_y)s%(range_month)s%(range_sec)s</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_tour_line_code" model="ir.sequence">
            <field name="name">Tour Line Code</field>
            <field name="code">master.tour.line.code</field>
            <field name="prefix">TRL%(range_y)s%(range_day)s%(range_sec)s</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_tour_room_code" model="ir.sequence">
            <field name="name">Tour Room Code</field>
            <field name="code">master.tour.room.code</field>
            <field name="prefix">TRM%(range_y)s%(range_month)s%(range_sec)s</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_request_tour" model="ir.sequence">
            <field name="name">Request Tour</field>
            <field name="code">request.tour</field>
            <field name="prefix">TRQ.%(range_y)s%(range_month)s%(range_day)s%(range_sec)s</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
