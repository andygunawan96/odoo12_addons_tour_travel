<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for transport.booking -->
        <record id="seq_reservation_periksain" model="ir.sequence">
            <field name="name">Reservation Periksain</field>
            <field name="code">tt.reservation.periksain</field>
            <field name="prefix">PK.%(y)s%(month)s%(day)s%(h24)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_timeslot_periksain" model="ir.sequence">
            <field name="name">Timeslot Periksain</field>
            <field name="code">tt.timeslot.periksain</field>
            <field name="prefix">TSP.%(day)s%(h24)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
