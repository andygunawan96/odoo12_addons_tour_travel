<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for transport.booking -->
        <record id="seq_reservation_mitrakeluarga" model="ir.sequence">
            <field name="name">Reservation Mitra Keluarga</field>
            <field name="code">tt.reservation.mitrakeluarga</field>
            <field name="prefix">MK.%(y)s%(month)s%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_timeslot_mitrakeluarga" model="ir.sequence">
            <field name="name">Timeslot Swab Express</field>
            <field name="code">tt.timeslot.mitrakeluarga</field>
            <field name="prefix">TMK.%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_price_list_mitrakeluarga" model="ir.sequence">
            <field name="name">Price List Swab Express</field>
            <field name="code">tt.price.list.mitrakeluarga</field>
            <field name="prefix">PMK.%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
