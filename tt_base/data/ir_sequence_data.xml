<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for transport.booking -->
        <record id="seq_customer" model="ir.sequence">
            <field name="name">Customer</field>
            <field name="code">tt.customer</field>
            <field name="prefix">CU.%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_customer" model="ir.sequence">
            <field name="name">Agent Citra</field>
            <field name="code">tt.agent.citra</field>
            <field name="prefix">CR.%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
