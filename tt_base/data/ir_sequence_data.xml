<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for transport.booking -->
        <record id="seq_customer" model="ir.sequence">
            <field name="name">Customer</field>
            <field name="code">tt.customer</field>
            <field name="prefix">CU.%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_payment_acquirer" model="ir.sequence">
            <field name="name">Payment Acquirer</field>
            <field name="code">pay.acq</field>
            <field name="prefix">PQR.%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_customer_parent" model="ir.sequence">
            <field name="name">Customer Parent</field>
            <field name="code">cust.par</field>
            <field name="prefix">CTP.%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_payment_unique_amount" model="ir.sequence">
            <field name="name">Payment Unique Amount</field>
            <field name="code">tt.payment.unique.amount</field>
            <field name="padding">3</field>
            <field name="number_next">100</field>
        </record>

        <record id="seq_upload_center" model="ir.sequence">
            <field name="name">Upload Center</field>
            <field name="code">tt.upload.center</field>
            <field name="prefix">UPC.%(day)s%(sec)s</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_pnr_quota" model="ir.sequence">
            <field name="name">PNR Quota</field>
            <field name="code">tt.pnr.quota</field>
            <field name="prefix">PQ.%(y)s%(month)s%(day)s%(sec)s</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_pnr_quota_price_list" model="ir.sequence">
            <field name="name">PNR Quota Price List</field>
            <field name="code">tt.pnr.quota.price.list</field>
            <field name="prefix">PQL.%(day)s%(sec)s</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_pnr_quota_price_package" model="ir.sequence">
            <field name="name">PNR Quota Price Package</field>
            <field name="code">tt.pnr.quota.price.package</field>
            <field name="prefix">PQP.%(day)s%(sec)s</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>
