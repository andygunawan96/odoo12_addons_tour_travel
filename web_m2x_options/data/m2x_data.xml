<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Setting Create and Edit portal by default -->
        <record model="ir.config_parameter" id="set_param_create">
            <field name="key">web_m2x_options.create</field>
            <field name="value">True</field>
        </record>
        <record model="ir.config_parameter" id="set_param_create_edit">
            <field name="key">web_m2x_options.create_edit</field>
            <field name="value">True</field>
        </record>
        <record model="ir.config_parameter" id="set_param_limit">
            <field name="key">web_m2x_options.limit</field>
            <field name="value">7</field>
        </record>
        <record model="ir.config_parameter" id="set_param_search_more">
            <field name="key">web_m2x_options.search_more</field>
            <field name="value">True</field>
        </record>
        <record model="ir.config_parameter" id="set_param_m2o_dialog">
            <field name="key">web_m2x_options.m2o_dialog</field>
            <field name="value">False</field>
        </record>
    </data>
</odoo>
