<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="activity_voucher_wizard" model="ir.ui.view">
			<field name="name">activity.voucher.wizard</field>
			<field name="model">activity.voucher.wizard</field>
			<field name="arch" type="xml">
				<form string="Message">
					<group>
						<group>
							<field name="user_email_add"/>
						</group>
						<group>
							<field name="provider_name"/>
							<field name="pnr"/>
						</group>
					</group>
					<footer>
						<button string="Send" name="resend_voucher_api" type="object" class="btn-primary" />
						<button string="Cancel" class="btn-default" special="cancel" />
					</footer>
				</form>
			</field>
		</record>
	</data>
</odoo>