<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="tt_pnr_quota_form_view_inherit" model="ir.ui.view">
        <field name="name">tt.pnr.quota.form.view.inherit</field>
        <field name="model">tt.pnr.quota</field>
        <field name="inherit_id" ref="tt_base.tt_pnr_quota_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='usage']" position="after">
                <page string="Ledger(s)">
                    <field name="ledger_ids"/>
                </page>
            </xpath>
        </field>
    </record>
</odoo>