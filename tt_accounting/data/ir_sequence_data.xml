<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="seq_top_up" model="ir.sequence">
        <field name="name">Top Up</field>
        <field name="code">tt.top.up</field>
        <field name="prefix">TU.%(range_year)s%(range_month)s%(range_day)s%(range_sec)s</field>
        <field name="padding">3</field>
    </record>

    <record id="seq_top_up_amount" model="ir.sequence">
        <field name="name">Top Up Amount</field>
        <field name="code">tt.top.up.amount</field>
        <field name="prefix">TUA.%(range_day)s%(range_sec)s</field>
        <field name="padding">3</field>
    </record>

    <record id="seq_adjustment" model="ir.sequence">
        <field name="name">Adjustment</field>
        <field name="code">tt.adjustment</field>
        <field name="prefix">ADJ.%(range_year)s%(range_month)s%(range_day)s%(range_sec)s</field>
        <field name="padding">3</field>
    </record>

    <record id="seq_refund" model="ir.sequence">
        <field name="name">Refund</field>
        <field name="code">tt.refund</field>
        <field name="prefix">RF.%(range_year)s%(range_month)s%(range_day)s%(range_sec)s</field>
        <field name="padding">3</field>
    </record>

</odoo>