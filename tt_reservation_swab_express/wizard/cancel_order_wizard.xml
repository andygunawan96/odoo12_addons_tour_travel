<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="cancel_order_swab_express_wizard_form_view" model="ir.ui.view">
        <field name="name">Cancel Order Wizard</field>
        <field name="model">cancel.order.swab.express.wizard</field>
        <field name="arch" type="xml">
            <form string="Cancel Order Wizard">
                <group>
                    <field name="booking_id"/>
                    <field name="cancellation_reason"/>
                </group>
                <footer>
                    <button name="cancel_order" string="Cancel Order" type="object" default_focus="1" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

    <record id="cancel_order_swab_express_wizard_action_view" model="ir.actions.act_window">
        <field name="name">Cancel Order Swab Express Wizard</field>
        <field name="res_model">cancel.order.swab.express.wizard</field>
        <field name="view_type">form</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

</odoo>