<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for transport.booking -->
        <record id="seq_reservation_insurance" model="ir.sequence">
            <field name="name">Reservation Insurance</field>
            <field name="code">tt.reservation.insurance</field>
            <field name="prefix">IR.%(y)s%(month)s%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_reservation_passenger_insurance" model="ir.sequence">
            <field name="name">Passenger Insurance</field>
            <field name="code">tt.reservation.passenger.insurance</field>
            <field name="prefix">PGI.%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
