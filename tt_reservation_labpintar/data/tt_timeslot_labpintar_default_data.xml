            <field name="single_supplement">25000</field>
            <field name="overtime_surcharge">50000</field>
            <field name="admin_fee_antigen_drivethru">10000</field>
            <field name="additional_price">20000</field>
            <field name="antigen_price_ids"
                   eval="[(6,0,[ref('tt_reservation_labpintar.tt_price_list_labpintar_antigen_1_person'),
