                   eval="[(6,0,[ref('tt_reservation_labpintar.tt_price_list_labpintar_pcr_priority_1_person'),
                             ])]"/>
            <field name="srbd_price_ids"
                   eval="[(6,0,[ref('tt_reservation_labpintar.tt_price_list_labpintar_srbd_1_person'),
                             ])]"/>
