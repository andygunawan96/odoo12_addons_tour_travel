<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for transport.booking -->
        <record id="seq_reservation_labpintar" model="ir.sequence">
            <field name="name">Reservation Lab Pintar</field>
            <field name="code">tt.reservation.labpintar</field>
            <field name="prefix">LP.%(y)s%(month)s%(day)s%(h24)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_timeslot_labpintar" model="ir.sequence">
            <field name="name">Timeslot Lab Pintar</field>
            <field name="code">tt.timeslot.labpintar</field>
            <field name="prefix">TLP.%(day)s%(h24)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_price_list_labpintar" model="ir.sequence">
            <field name="name">Price List Lab Pintar</field>
            <field name="code">tt.price.list.labpintar</field>
            <field name="prefix">PLP.%(day)s%(h24)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
