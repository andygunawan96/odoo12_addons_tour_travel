<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for transport.booking -->
        <record id="seq_reservation_sentramedika" model="ir.sequence">
            <field name="name">Reservation Sentra Medika</field>
            <field name="code">tt.reservation.sentramedika</field>
            <field name="prefix">SM.%(y)s%(month)s%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_timeslot_sentramedika" model="ir.sequence">
            <field name="name">Timeslot Sentra Medika</field>
            <field name="code">tt.timeslot.sentramedika</field>
            <field name="prefix">TSM.%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_price_list_sentramedika" model="ir.sequence">
            <field name="name">Price List Sentra Medika</field>
            <field name="code">tt.price.list.sentramedika</field>
            <field name="prefix">PSM.%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
