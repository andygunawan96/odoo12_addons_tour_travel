                <field name="name"/>
                <field name="description"/>
                <field name="commission_percentage"/>
                <field name="commission_minimum"/>
                <field name="commission_amount"/>
                <field name="commission_route"/>
                <field name="commission_segment"/>
                <field name="commission_pax"/>
                <field name="commission_infant"/>
                <field name="active" widget="boolean_toggle"/>
            </tree>
        </field>
    </record>

    <record id="agent_pricing_upline_view_search" model="ir.ui.view">
        <field name="name">agent.pricing.upline.view.search</field>
        <field name="model">tt.agent.pricing.upline</field>
        <field name="arch" type="xml">
        <search>
            <field name="name"/>
            <filter name="inactive" string="Archived" domain="[('active','=',False)]"/>
        </search>
        </field>
    </record>

</odoo>
