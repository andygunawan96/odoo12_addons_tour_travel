<odoo>
    <!--  Agent Pricing  -->
    <record id="agent_pricing_view_form" model="ir.ui.view">
        <field name="name">agent.pricing.view.form</field>
        <field name="model">tt.agent.pricing</field>
        <field name="arch" type="xml">
            <form>
                <header>
                    <button string="Test Get Data" type="object" name="test_get_data" groups="base.group_no_one"/>
                    <button string="Action Compute All Name" type="object" name="action_compute_all_name" groups="base.group_no_one"/>
<!--                    <button string="Action Set All Agent Type Value to One2Many" type="object" name="action_set_all_agent_type" groups="base.group_no_one"/>-->
                </header>
                <sheet>
                    <div class="oe_button_box" name="button_box">
                        <button name="toggle_active" type="object" class="oe_stat_button" icon="fa-archive">
                                <field name="active" widget="boolean_button" options='{"terminology": "archive"}'/>
                        </button>
                    </div>
                    <div class="oe_title">
                        <label for="name" class="oe_edit_only"/>
                        <h1><field name="name"/></h1>
                    </div>
                    <group string="Description">
                        <field name="description" nolabel="1"/>
                    </group>
                    <group>
                        <group>
                            <field name="sequence"/>
<!--                            <field name="agent_type_id"/>-->
                        </group>
                        <group>
                            <field name="state" widget="radio"/>
                        </group>
                    </group>
                    <notebook>
                        <page string="Providers and Carriers" name="provider_types">
                            <group>
                                <group>
                                    <field name="provider_type_access_type" widget="radio"/>
                                </group>
                                <group>
                                </group>
                            </group>
                            <group string="Provider Type List">
                                <field name="provider_type_ids" nolabel="1"/>
                            </group>
                            <group>
                                <group>
                                    <field name="provider_access_type" widget="radio"/>
                                </group>
                                <group>
                                </group>
                            </group>
                            <group string="Provider List">
                                <field name="provider_ids" nolabel="1"/>
                            </group>
                            <group>
                                <group>
                                    <field name="carrier_access_type" widget="radio"/>
                                </group>
                                <group>
                                </group>
                            </group>
                            <group string="Carrier List">
                                <field name="carrier_ids" nolabel="1"/>
                            </group>
                        </page>
                        <page string="Agents" name="agent_types">
                            <group>
                                <group>
                                    <field name="agent_type_access_type" widget="radio"/>
                                </group>
                                <group>
                                </group>
                            </group>
                            <group string="Agent Type List">
                                <field name="agent_type_ids" nolabel="1"/>
                            </group>
                            <group>
                                <group>
                                    <field name="agent_access_type" widget="radio"/>
                                </group>
                                <group>
                                </group>
                            </group>
                            <group string="Agent List">
                                <field name="agent_ids" nolabel="1"/>
                            </group>
                        </page>
                    </notebook>
                    <hr/>
                    <group string="Rules">
                        <field name="line_ids" nolabel="1" context="{'default_pricing_id': id}"/>
                    </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
        </field>
    </record>

    <record id="agent_pricing_view_tree" model="ir.ui.view">
        <field name="name">agent.pricing.view.tree</field>
        <field name="model">tt.agent.pricing</field>
        <field name="arch" type="xml">
            <tree decoration-muted="active == False">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
                <field name="description"/>
                <field name="active" widget="boolean_toggle"/>
            </tree>
        </field>
    </record>

    <record id="agent_pricing_view_search" model="ir.ui.view">
        <field name="name">agent.pricing.view.search</field>
        <field name="model">tt.agent.pricing</field>
        <field name="arch" type="xml">
        <search>
            <field name="name"/>
            <filter name="inactive" string="Archived" domain="[('active','=',False)]"/>
        </search>
        </field>
    </record>

    <record id="agent_pricing_action" model="ir.actions.act_window">
        <field name="name">Agent Pricing</field>
        <field name="res_model">tt.agent.pricing</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{'active_test': False}</field>
    </record>

    <menuitem id="agent_pricing_menu" parent="tt_engine_pricing_v2.menu_tour_travel_pricing_booking"
              groups="tt_base.group_pricing_provider_level_1" name="Agent Pricing" action="agent_pricing_action"
              sequence="20"/>

