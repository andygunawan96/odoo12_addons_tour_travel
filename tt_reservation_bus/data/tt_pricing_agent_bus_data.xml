<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for default train pricing agent -->
        <!--        Pricing Agent Line-->
        <record id="tt_pricing_agent_default_citra_bus_line_HO" model="tt.pricing.agent.line">
            <field name="name">HO</field>
            <field name="sequence">10</field>
            <field name="agent_type_id" ref="tt_base.agent_type_ho"/>
            <field name="basic_amount">30</field>
        </record>


        <record id="tt_pricing_agent_default_japro_bus_line_citra" model="tt.pricing.agent.line">
            <field name="name">HO</field>
            <field name="sequence">10</field>
            <field name="agent_type_id" ref="tt_base.agent_type_citra"/>
            <field name="basic_amount">2.5</field>
        </record>

        <record id="tt_pricing_agent_default_japro_bus_line_HO" model="tt.pricing.agent.line">
            <field name="name">HO</field>
            <field name="sequence">15</field>
            <field name="agent_type_id" ref="tt_base.agent_type_ho"/>
            <field name="basic_amount">25</field>
        </record>

        <record id="tt_pricing_agent_default_btc_bus_line_HO" model="tt.pricing.agent.line">
            <field name="name">HO</field>
            <field name="sequence">10</field>
            <field name="agent_type_id" ref="tt_base.agent_type_ho"/>
            <field name="basic_amount">100</field>
        </record>

        <!--        Pricing Agent-->
        <record id="tt_pricing_agent_default_ho_bus" model="tt.pricing.agent">
            <field name="agent_type_id" ref="tt_base.agent_type_ho"/>
            <field name="provider_type_id" ref="tt_provider_type_bus"/>
            <field name="basic_amount">100</field>
            <field name="currency_id" ref="base.IDR"/>
            <field name="sequence">999</field>
            <field name="active" eval="False"/>
        </record>

        <record id="tt_pricing_agent_default_citra_bus" model="tt.pricing.agent">
            <field name="agent_type_id" ref="tt_base.agent_type_citra"/>
            <field name="provider_type_id" ref="tt_provider_type_bus"/>
            <field name="basic_amount">70</field>
            <field name="currency_id" ref="base.IDR"/>
            <field name="sequence">999</field>
            <field name="active" eval="False"/>
            <field name="line_ids" eval="[(6,0, [
                                        ref('tt_pricing_agent_default_citra_bus_line_HO')
                                        ])]"/>
        </record>

        <record id="tt_pricing_agent_default_japro_bus" model="tt.pricing.agent">
            <field name="agent_type_id" ref="tt_base.agent_type_japro"/>
            <field name="provider_type_id" ref="tt_provider_type_bus"/>
            <field name="basic_amount">70</field>
            <field name="loop_level">1</field>
            <field name="currency_id" ref="base.IDR"/>
            <field name="sequence">999</field>
            <field name="active" eval="False"/>
            <field name="line_ids" eval="[(6,0, [
                                        ref('tt_pricing_agent_default_japro_bus_line_citra'),
                                        ref('tt_pricing_agent_default_japro_bus_line_HO')
                                        ])]"/>
        </record>

        <record id="tt_pricing_agent_default_btc_bus" model="tt.pricing.agent">
            <field name="agent_type_id" ref="tt_base.agent_type_btc"/>
            <field name="provider_type_id" ref="tt_provider_type_bus"/>
            <field name="basic_amount">0</field>
            <field name="currency_id" ref="base.IDR"/>
            <field name="sequence">999</field>
            <field name="active" eval="False"/>
            <field name="line_ids" eval="[(6,0, [
                                        ref('tt_pricing_agent_default_btc_bus_line_HO')
                                        ])]"/>
        </record>

    </data>
</odoo>