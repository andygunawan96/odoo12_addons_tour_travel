<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for transport.booking -->
        <record id="seq_master_event" model="ir.sequence">
            <field name="name">Event Master</field>
            <field name="code">tt.master.event</field>
            <field name="prefix">EVT.%(y)s%(month)s%(day)s%(h24)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_event_pnr" model="ir.sequence">
            <field name="name">PNR</field>
            <field name="code">pnr_sequence</field>
            <field name="prefix">EV.%(y)s%(month)s%(day)s%(h24)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_event_option" model="ir.sequence">
            <field name="name">Event Master</field>
            <field name="code">tt.event.option</field>
            <field name="prefix">OPT.%(h24)s</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for transport.booking -->
        <record id="seq_reservation_event" model="ir.sequence">
            <field name="name">Reservation Event</field>
            <field name="code">tt.reservation.event</field>
            <field name="prefix">VT.%(y)s%(month)s%(day)s%(h24)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
