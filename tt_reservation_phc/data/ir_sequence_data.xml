<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for transport.booking -->
        <record id="seq_reservation_phc" model="ir.sequence">
            <field name="name">Reservation PHC</field>
            <field name="code">tt.reservation.phc</field>
            <field name="prefix">PH.%(y)s%(month)s%(day)s%(h24)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_timeslot_phc" model="ir.sequence">
            <field name="name">Timeslot phc</field>
            <field name="code">tt.timeslot.phc</field>
            <field name="prefix">TSC.%(day)s%(h24)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_reservation_passenger_phc" model="ir.sequence">
            <field name="name">Passenger phc</field>
            <field name="code">tt.reservation.passenger.phc</field>
            <field name="prefix">PGH.%(day)s%(h24)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
