<odoo>
    <record id="tt_reservation_airline_banner_tree_view" model="ir.ui.view">
        <field name="name">tt.banner.airline.tree.view</field>
        <field name="model">tt.banner.airline</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="description"/>
                <field name="banner_color"/>
                <field name="text_color"/>
                <field name="journey_id" groups="tt_base.group_tt_tour_travel"/>
            </tree>
        </field>
    </record>

</odoo>