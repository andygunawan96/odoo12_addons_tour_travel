<odoo>
    <record id="tt_reservation_airline_for_ho" model="ir.rule">
        <field name="name">Reservation Airline For HO</field>
        <field name="model_id" ref="model_tt_reservation_airline"/>
        <field name="domain_force">[('ho_id','=',user.ho_id.id)]</field>
        <field name="groups" eval="[(4,ref('tt_base.group_tt_tour_travel_operator'))]"/>
    </record>

    <record id="tt_reservation_airline_for_ho_user" model="ir.rule">
        <field name="name">Reservation Airline For HO User</field>
        <field name="model_id" ref="model_tt_reservation_airline"/>
        <field name="domain_force">[('ho_id','=',user.ho_id.id)]</field>
        <field name="groups" eval="[(4,ref('tt_base.group_tt_tour_travel'))]"/>
    </record>

    <record id="tt_reservation_airline_for_agent" model="ir.rule">
        <field name="name">Reservation Airline For Agent</field>
        <field name="model_id" ref="model_tt_reservation_airline"/>
        <field name="domain_force">[('agent_id','=',user.agent_id.id)]</field>  <!--['|',('agent_id','=',user.agent_id.id),('sub_agent_id','=',user.agent_id.id)]-->  <!--[('agent_id','=',user.agent_id.id)]-->
        <field name="groups" eval="[(4,ref('tt_base.group_tt_agent_user_operator'))]"/>  <!--,(4,ref('tt_base.group_tt_sub_agent_user_operator'))-->
    </record>

    <record id="tt_reservation_airline_for_agent_user" model="ir.rule">
        <field name="name">Reservation Airline For Agent User</field>
        <field name="model_id" ref="model_tt_reservation_airline"/>
        <field name="domain_force">[('agent_id','=',user.agent_id.id)]</field>  <!--['|',('agent_id','=',user.agent_id.id),('sub_agent_id','=',user.agent_id.id)]-->  <!--[('agent_id','=',user.agent_id.id)]-->
        <field name="groups" eval="[(4,ref('tt_base.group_tt_agent_user'))]"/>  <!--,(4,ref('tt_base.group_tt_sub_agent_user_operator'))-->
    </record>

    <record id="tt_reservation_airline_for_corpor_user" model="ir.rule">
        <field name="name">Reservation Airline For Corporate User</field>
        <field name="model_id" ref="model_tt_reservation_airline"/>
        <field name="domain_force">[('agent_id','=',user.agent_id.id),('customer_parent_id','=',user.customer_parent_id.id)]</field>  <!--['|',('agent_id','=',user.agent_id.id),('sub_agent_id','=',user.agent_id.id)]-->  <!--[('agent_id','=',user.agent_id.id)]-->
        <field name="groups" eval="[(4,ref('tt_base.group_tt_corpor_user'))]"/>  <!--,(4,ref('tt_base.group_tt_sub_agent_user_operator'))-->
    </record>

    <record id="tt_provider_airline_for_ho" model="ir.rule">
        <field name="name">Provider Airline For HO</field>
        <field name="model_id" ref="model_tt_provider_airline"/>
        <field name="domain_force">[('ho_id','=',user.ho_id.id)]</field>
        <field name="groups" eval="[(4,ref('tt_base.group_tt_tour_travel_operator'))]"/>
    </record>

    <record id="tt_provider_airline_for_ho_user" model="ir.rule">
        <field name="name">Provider Airline For HO User</field>
        <field name="model_id" ref="model_tt_provider_airline"/>
        <field name="domain_force">[('ho_id','=',user.ho_id.id)]</field>
        <field name="groups" eval="[(4,ref('tt_base.group_tt_tour_travel'))]"/>
    </record>
</odoo>