<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for transport.booking -->
        <record id="seq_reservation_airline" model="ir.sequence">
            <field name="name">Reservation Airline</field>
            <field name="code">tt.reservation.airline</field>
            <field name="prefix">AL.%(y)s%(month)s%(day)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
