<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequences for default airline pricing agent -->
        <!--        Pricing Agent Line-->
        <record id="tt_pricing_agent_default_citra_airline_line_HO" model="tt.pricing.agent.line">
            <field name="name">HO</field>
            <field name="sequence">10</field>
            <field name="agent_type_id" ref="tt_base.agent_type_ho"/>
            <field name="basic_amount">30</field>
        </record>


        <record id="tt_pricing_agent_default_japro_airline_line_citra" model="tt.pricing.agent.line">
            <field name="name">HO</field>
            <field name="sequence">10</field>
            <field name="agent_type_id" ref="tt_base.agent_type_citra"/>
            <field name="basic_amount">2.5</field>
        </record>

        <record id="tt_pricing_agent_default_japro_airline_line_HO" model="tt.pricing.agent.line">
            <field name="name">HO</field>
            <field name="sequence">15</field>
            <field name="agent_type_id" ref="tt_base.agent_type_ho"/>
            <field name="basic_amount">25</field>
        </record>

        <record id="tt_pricing_agent_default_btc_airline_line_HO" model="tt.pricing.agent.line">
            <field name="name">HO</field>
            <field name="sequence">10</field>
            <field name="agent_type_id" ref="tt_base.agent_type_ho"/>
            <field name="basic_amount">100</field>
        </record>

        <!--        Pricing Agent-->
        <record id="tt_pricing_agent_default_ho_airline" model="tt.pricing.agent">
            <field name="agent_type_id" ref="tt_base.agent_type_ho"/>
            <field name="provider_type_id" ref="tt_provider_type_airline"/>
            <field name="basic_amount">100</field>
            <field name="currency_id" ref="base.IDR"/>
        </record>

        <record id="tt_pricing_agent_default_citra_airline" model="tt.pricing.agent">
            <field name="agent_type_id" ref="tt_base.agent_type_citra"/>
            <field name="provider_type_id" ref="tt_provider_type_airline"/>
            <field name="basic_amount">70</field>
            <field name="currency_id" ref="base.IDR"/>
            <field name="line_ids" eval="[(6,0, [
                                        ref('tt_pricing_agent_default_citra_airline_line_HO')
                                        ])]"/>
        </record>

        <record id="tt_pricing_agent_default_japro_airline" model="tt.pricing.agent">
            <field name="agent_type_id" ref="tt_base.agent_type_japro"/>
            <field name="provider_type_id" ref="tt_provider_type_airline"/>
            <field name="basic_amount">70</field>
            <field name="loop_level">1</field>
            <field name="currency_id" ref="base.IDR"/>
            <field name="line_ids" eval="[(6,0, [
                                        ref('tt_pricing_agent_default_japro_airline_line_citra'),
                                        ref('tt_pricing_agent_default_japro_airline_line_HO')
                                        ])]"/>
        </record>

        <record id="tt_pricing_agent_default_btc_airline" model="tt.pricing.agent">
            <field name="agent_type_id" ref="tt_base.agent_type_btc"/>
            <field name="provider_type_id" ref="tt_provider_type_airline"/>
            <field name="basic_amount">0</field>
            <field name="currency_id" ref="base.IDR"/>
            <field name="line_ids" eval="[(6,0, [
                                        ref('tt_pricing_agent_default_btc_airline_line_HO')
                                        ])]"/>
        </record>

    </data>
</odoo>