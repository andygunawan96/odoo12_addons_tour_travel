<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Sequences for agent.registration -->
        <record id="seq_passport_number" model="ir.sequence">
            <field name="name">Reservation Passport</field>
            <field name="code">tt.reservation.passport</field>
            <field name="prefix">PS.%(y)s%(month)s%(day)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
