<odoo>
    <data noupdate="1">
        <record id="request_offline_seq" model="ir.sequence">
            <field name="name">Request Offline sequence</field>
            <field name="code">issued.offline</field>
            <field name="prefix">RO.%(range_year)s%(range_month)s%(range_day)s</field>
            <field eval="3" name="padding"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>
    </data>
</odoo>